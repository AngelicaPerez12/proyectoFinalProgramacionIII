<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="8" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectoprogra.controllers.Cliente.TicketController" style="-fx-padding:20;">

    <ImageView fx:id="imgLogo" fitWidth="120" fitHeight="60" preserveRatio="true"/>
    <Label fx:id="lblTitulo" style="-fx-font-size:18px; -fx-font-weight:bold;" />
    <Label fx:id="lblId" />
    <Label fx:id="lblCliente" />
    <Label fx:id="lblCorreo" />
    <Label fx:id="lblPastel" />
    <Label fx:id="lblTipoTamano" />
    <Label fx:id="lblDescripcion" wrapText="true" />
    <Label fx:id="lblFechaEntrega" />
    <Label fx:id="lblPrecio" style="-fx-font-size:16px; -fx-font-weight:bold;" />
    <Button text="Cerrar" onAction="#cerrar" />
</VBox>