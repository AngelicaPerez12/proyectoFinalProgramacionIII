<AnchorPane prefHeight="432.0" styleClass="pane" prefWidth="640.0" xmlns="http://javafx.com/javafx/25"  stylesheets="Styles/dashadmin.css" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectoprogra.controllers.DashboardAdminController">
