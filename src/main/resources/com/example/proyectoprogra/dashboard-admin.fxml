            </ImageView>
            <Button fx:id="btnpastel" mnemonicParsing="false" onAction="#inforPasteles" prefHeight="25.0" prefWidth="91.0" style="-fx-background-color: transparent;" text="Pasteles" translateX="20.0" translateY="15.0" />
            <ImageView fitHeight="25.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" translateX="10.0" translateY="48.0">
               <image>
                  <Image url="@Imagenes/checkout.png" />
