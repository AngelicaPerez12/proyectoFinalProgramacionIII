            </ImageView>
            <Button fx:id="btnhistorial" mnemonicParsing="false" onAction="#historial" prefHeight="25.0" prefWidth="91.0" style="-fx-background-color: transparent;" text="Historial" translateX="15.0" translateY="15.0" />
